{% extends "pizzaSiteBundle::base.html.twig" %}

{% block title %}Phone{% endblock %}

{% block content %}
    <h1>Phone</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Phonenumber</th>
                <td>{{ entity.phonenumber }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('phone') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('phone_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
